#include<stdlib.h>
#include<stdio.h>
#include<math.h>

main()

{
      float a,b,c;
      
      printf("Inserisci il primo cateto");
      scanf("%f", &a);
      
      printf("Inserisci il secondo cateto");
      scanf("%f", &b);
      
      c=sqrt((a*a)+(b*b));
      
      printf("L'ipotenusa e' %f",c);
      
      system("pause");
      
      
      
      }
